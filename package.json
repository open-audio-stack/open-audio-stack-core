{
  "name": "@open-audio-stack/core",
  "version": "0.1.0",
  "description": "Open-source audio plugin management software",
  "type": "module",
  "main": "./build/index.js",
  "exports": {
    "node": "./build/index.js",
    "default": "./build/index-browser.js"
  },
  "types": "./build/index.d.ts",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsx ./src/index.ts",
    "format": "prettier . --write",
    "lint": "eslint .",
    "start": "node build/index.js",
    "test": "vitest run ./tests --hook-timeout=60000 --test-timeout=60000 --no-file-parallelism",
    "test:watch": "vitest ./tests"
  },
  "author": "kmturley",
  "license": "cc0-1.0",
  "keywords": [
    "open",
    "audio",
    "stack",
    "plugin",
    "package",
    "manager"
  ],
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/7zip-min": "^1.3.3",
    "@types/adm-zip": "^0.5.5",
    "@types/fs-extra": "^11.0.4",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.7.8",
    "@types/semver": "^7.5.8",
    "@vitest/coverage-v8": "^2.1.3",
    "eslint": "^9.12.0",
    "globals": "^15.2.0",
    "prettier": "^3.2.5",
    "tsx": "^4.10.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.9.0",
    "vitest": "^2.1.3"
  },
  "dependencies": {
    "@vscode/sudo-prompt": "^9.3.1",
    "7zip-min": "^1.4.5",
    "adm-zip": "^0.5.16",
    "chalk": "^5.3.0",
    "fs-extra": "^11.2.0",
    "glob": "^11.0.0",
    "js-yaml": "^4.1.0",
    "semver": "^7.6.3",
    "tar": "^7.4.3",
    "zod": "^3.23.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/open-audio-stack/open-audio-stack-core.git"
  }
}
